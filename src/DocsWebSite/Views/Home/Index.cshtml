@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta charset="UTF-8">
    <link rel="stylesheet" href="~/libs/docsify/themes/vue.css">
</head>
<body>
    <div id="app"></div>
    <script>
        window.$docsify = {
            loadSidebar: true,          
            name: 'Docs Site',
            alias: {
                '/.*/_sidebar.md': '/_sidebar.md'
            },
            subMaxLevel: 3,
            //coverpage: true,
            themeColor: 'orangered',
            autoHeader: true
        }
    </script>
    <script src="~/libs/docsify/docsify.min.js"></script>
    <script src="~/libs/docsify/plugins/search.min.js"></script>
    <script src="~/libs/docsify-pagenation/docsify-pagination.min.js"></script>
    <script src ="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-csharp.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-powershell.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-plsql.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-sql.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-batch.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-aspnet.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-cpp.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-css.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-json.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/prismjs@1.22.0/components/prism-typescript.min.js"></script>
</body>
</html>